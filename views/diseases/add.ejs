<form method="POST" action="/diseases">
	<div class="form-group">
		<div>
		<label>Name</label>
		<input type="text" name="name" placeholder="disease name goes here" class="form-control">
		</div>
		<div class="form-group">
		<label>Description</label>
		<textarea name="description" class="form-control"></textarea>
		</div>
		<div>
		<label>Severity</label>
		<input type="number" name="severity" placeholder="1-10" class="form-control">
		</div>
		<div>
		<label>Transmission</label>
		<input type="text" name="transmisson" placeholder="transmisson type" class="form-control">
		</div>
		<div>
		<label>Symptoms</label>
		<input type="text" name="symptoms" placeholder="Symptoms" class="form-control">
		</div>
	</div>
	<input type="submit" class="btn btn-success">
</form>
